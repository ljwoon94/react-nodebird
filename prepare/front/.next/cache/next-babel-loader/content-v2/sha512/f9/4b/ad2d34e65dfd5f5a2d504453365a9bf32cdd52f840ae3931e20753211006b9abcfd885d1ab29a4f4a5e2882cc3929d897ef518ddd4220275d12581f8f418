;AAAA;AAAA;AAAA;AALR;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YAjER;AAAA;AAAA;AAAA;AAAA;AAAA,UADJ;AAyFH,CAxID;;AA0IAhC,QAAQ,CAACoC,SAAT,GAAqB;AACjBnC,EAAAA,IAAI,EAAEpB,SAAS,CAACwD,KAAV,CAAgB;AAClB9B,IAAAA,EAAE,EAAE1B,SAAS,CAACyD,MADI;AAElBb,IAAAA,IAAI,EAAE5C,SAAS,CAAC0D,MAFE;AAGlBP,IAAAA,OAAO,EAAEnD,SAAS,CAAC2D,MAHD;AAIlBV,IAAAA,QAAQ,EAAEjD,SAAS,CAAC2D,MAJF;AAKlBP,IAAAA,QAAQ,EAAEpD,SAAS,CAAC4D,OAAV,CAAkB5D,SAAS,CAAC0D,MAA5B,CALQ;AAMlBf,IAAAA,MAAM,EAAE3C,SAAS,CAAC4D,OAAV,CAAkB5D,SAAS,CAAC0D,MAA5B,CANU;AAOlBnB,IAAAA,MAAM,EAAEvC,SAAS,CAAC4D,OAAV,CAAkB5D,SAAS,CAAC0D,MAA5B,CAPU;AAQlBb,IAAAA,SAAS,EAAE7C,SAAS,CAACyD,MARH;AASlBV,IAAAA,OAAO,EAAE/C,SAAS,CAAC6D,QAAV,CAAmB7D,SAAS,CAAC8D,GAA7B;AATS,GAAhB,EAUHC;AAXc,CAArB;AAcA,eAAe5C,QAAf","sourcesContent":["import React, { useCallback, useState } from 'react';\r\nimport { Card, Popover, Button, Avatar, List, Comment } from 'antd';\r\nimport PropTypes from 'prop-types';\r\nimport { RetweetOutlined, HeartOutlined, MessageOutlined, EllipsisOutlined, HeartTwoTone, } from '@ant-design/icons';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport PostImages from './PostImages';\r\nimport CommentForm from './CommentForm';\r\nimport PostCardContent from './PostCardContent';\r\nimport { LIKE_POST_REQUEST, REMOVE_POST_REQUEST, UNLIKE_POST_REQUEST, RETWEET_REQUEST } from '../reducers/post';\r\nimport FollowButton from './FollowButton';\r\nimport Link from 'next/link';\r\nimport moment from 'moment';\r\n// moment 날짜 api\r\n\r\nmoment.locale('ko');\r\n\r\nconst PostCard = ({ post }) => {\r\n    const dispatch = useDispatch();\r\n    const { removePostLoading } = useSelector((state) => state.post);\r\n    const [commentFormOpened, setCommentFormOpened] = useState(false);\r\n    const id = useSelector((state) => state.user.me?.id);\r\n\r\n    const onLike = useCallback(() => {\r\n        if(!id){\r\n            return alert('로그인이 필요합니다.');\r\n        }\r\n        return dispatch({\r\n            type: LIKE_POST_REQUEST,\r\n            data: post.id,\r\n        });\r\n    }, [id]);\r\n    const onUnLike = useCallback(() => {\r\n        if(!id){\r\n            return alert('로그인이 필요합니다.');\r\n        }\r\n        return dispatch({\r\n            type: UNLIKE_POST_REQUEST,\r\n            data: post.id,\r\n        });\r\n    }, [id]);\r\n    const onToggleComment = useCallback(() => {\r\n        setCommentFormOpened((prev) => !prev);\r\n    }, []);\r\n\r\n    const onRemovePost = useCallback(() => {\r\n        return dispatch({\r\n            type: REMOVE_POST_REQUEST,\r\n            data: post.id,\r\n        });\r\n    }, []);\r\n\r\n    const onRetweet = useCallback(()=>{\r\n        if(!id){\r\n            return alert('로그인이 필요합니다.');\r\n        }\r\n        return dispatch({\r\n            type: RETWEET_REQUEST,\r\n            data: post.id,\r\n        });\r\n    }, [id]);\r\n\r\n    \r\n    const liked = post.Likers.find((v) => v.id === id);\r\n    return (\r\n        <div style={{ marginBottom: 20 }}>\r\n            <Card\r\n                cover={post.Images[0] && <PostImages images={post.Images} />}\r\n                actions={[\r\n                    <RetweetOutlined key=\"retweet\" onClick={onRetweet} />,\r\n                    liked\r\n                        ? <HeartTwoTone twoToneColor=\"#eb2f96\" key=\"heart\" onClick={onUnLike} />\r\n                        : <HeartOutlined key=\"heart\" onClick={onLike} />,\r\n                    <MessageOutlined key=\"comment\" onClick={onToggleComment} />,\r\n                    <Popover key=\"more\" content={(\r\n                        <Button.Group>\r\n                            {id && post.User.id === id\r\n                                ? (\r\n                                    <>\r\n                                        <Button>수정</Button>\r\n                                        <Button type=\"danger\" onClick={onRemovePost}\r\n                                            loading={removePostLoading}>삭제</Button>\r\n                                    </>\r\n                                )\r\n                                : <Button>신고</Button>}\r\n                        </Button.Group>\r\n                    )}>\r\n                        <EllipsisOutlined />\r\n                    </Popover>,\r\n                ]}\r\n                title={post.RetweetId ? `${post.User.nickname}님이 리트윗하셨습니다.` : null}\r\n                extra={id && <FollowButton post={post} />}\r\n            //extra는 우측상단에 공간을 만들어준다.\r\n            >\r\n                {post.RetweetId && post.Retweet\r\n                ? (\r\n                    <Card\r\n                        cover={post.Retweet.Images[0] && <PostImages images={post.Retweet.Images} />}>\r\n                        <div style={{float:'right'}}>{moment(post.createAt).format('YYYY.MM.DD')}</div>\r\n                        <Card.Meta\r\n                            avatar={(\r\n                                <Link href={`/user/${post.Retweet.User.id}`}>\r\n                                    <a><Avatar>{post.Retweet.User.nickname[0]}</Avatar></a>\r\n                                </Link>\r\n                            )}\r\n                            title={post.Retweet.User.nickname}\r\n                            description={(\r\n                                <PostCardContent postData={post.Retweet.content} />\r\n                            )}\r\n                        />\r\n\r\n                    </Card>\r\n                )\r\n                : (\r\n                    <>\r\n                    <div style={{float:'right'}}>{moment(post.createAt).format('YYYY.MM.DD')}</div>\r\n                    <Card.Meta\r\n                    avatar={(\r\n                        <Link href={`/user/${post.User.id}`}>\r\n                            <a><Avatar>{post.User.nickname[0]}</Avatar></a>\r\n                        </Link>\r\n                    )}\r\n                        title={post.User.nickname}\r\n                        description={<PostCardContent postData={post.content} />}\r\n                    />\r\n                    </>\r\n                )}\r\n\r\n            </Card >\r\n            {commentFormOpened && (\r\n                <div>\r\n                    <CommentForm post={post} />\r\n                    <List\r\n                        header={`${post.Comments.length}개의 댓글`}\r\n                        itemLayout=\"horizontal\"\r\n                        dataSource={post.Comments}\r\n                        renderItem={(item) => (\r\n                            <li>\r\n                                <Comment\r\n                                    author={item.User.nickname}\r\n                                    avatar={<Link href={`/user/${item.User.id}`}><a><Avatar>{item.User.nickname[0]}</Avatar></a></Link>}\r\n                                    content={item.content}\r\n                                />\r\n\r\n\r\n                            </li>\r\n                        )}\r\n                    />\r\n                </div>)}\r\n            {/* <CommentForm /> */}\r\n            {/* <Comments /> */}\r\n        </div >\r\n    )\r\n};\r\n\r\nPostCard.propTypes = {\r\n    post: PropTypes.shape({\r\n        id: PropTypes.number,\r\n        User: PropTypes.object,\r\n        content: PropTypes.string,\r\n        createAt: PropTypes.string,\r\n        Comments: PropTypes.arrayOf(PropTypes.object),\r\n        Images: PropTypes.arrayOf(PropTypes.object),\r\n        Likers: PropTypes.arrayOf(PropTypes.object),\r\n        RetweetId: PropTypes.number,\r\n        Retweet: PropTypes.objectOf(PropTypes.any),\r\n    }).isRequired,\r\n}\r\n\r\nexport default PostCard;\r\n"]},"metadata":{},"sourceType":"module"}